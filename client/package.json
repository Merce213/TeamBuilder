{
	"name": "client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"build:css": "sass --style compressed src/styles/scss/main.scss src/styles/css/main.min.css",
		"watch:css": "sass --watch src/styles/scss/main.scss src/styles/css/main.min.css",
		"purge:css": "npx purgecss --content ./src/**/*.tsx --css src/styles/css/main.min.css --output src/styles/css/main.min.css",
		"lint": "eslint .",
		"preview": "vite preview"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.62.11",
		"axios": "^1.7.9",
		"lucide-react": "^0.469.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^7.1.1",
		"sonner": "^1.7.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.17.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.16",
		"globals": "^15.14.0",
		"sass": "^1.83.0",
		"typescript": "~5.6.2",
		"typescript-eslint": "^8.18.2",
		"vite": "^6.0.5"
	}
}
